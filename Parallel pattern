class Parallel{
    public static void main(String args[]){
        int x = 1;
        int n = 3;
        for(int i = 1;i<=n;i++){
            for(int j = 1;j<=i;j++){
                System.out.print(x);
            }
            System.out.println();
        }
        for(int i = n-1;i>=1;i--){
            for(int j = 1;j<=i;j++){
                System.out.print(x);
            }
            System.out.println();
        }
    }
}
